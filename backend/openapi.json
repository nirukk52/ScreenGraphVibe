{
  "openapi": "3.0.3",
  "info": {
    "title": "ScreenGraphVibe Backend API",
    "version": "0.1.0"
  },
  "components": {
    "schemas": {}
  },
  "paths": {
    "/healthz": {
      "get": {
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "ok",
                        "error"
                      ]
                    },
                    "requestId": {
                      "type": "string"
                    },
                    "trace_id": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "status",
                    "requestId",
                    "trace_id"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/graph/{runId}": {
      "get": {
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "path",
            "name": "runId",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Default Response"
          }
        }
      }
    },
    "/app-launch-configs": {
      "get": {
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "apkPath": {
                            "type": "string"
                          },
                          "packageName": {
                            "type": "string"
                          },
                          "appActivity": {
                            "type": "string"
                          },
                          "appiumServerUrl": {
                            "type": "string"
                          },
                          "isDefault": {
                            "type": "boolean"
                          }
                        },
                        "required": [
                          "id",
                          "name",
                          "apkPath",
                          "packageName",
                          "appActivity",
                          "appiumServerUrl"
                        ]
                      }
                    },
                    "trace_id": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "items",
                    "trace_id"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
